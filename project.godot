; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="nightmath"
config/tags=PackedStringArray("ass_we_can", "fisting_for_300$")
run/main_scene="res://scen/gui/splash_screens.tscn"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=1000
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="uid://bju2pir7em7lb"
boot_splash/minimum_display_time=100

[autoload]

Event="*res://scripts/event.gd"
MPIO="*res://addons/MultiplayCore/MPIO.gd"
InventoryManager="*res://scripts/Inventory_manager.gd"
LoadResouce="*res://resources/LoadResouce.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Shaker="*res://addons/shaker/src/Shaker.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0
window/bg_color=Color(0.14, 0.14, 0.14, 1)
window/name="3-nat"

[dotnet]

project/assembly_name="Роб"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_nodes/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/shaker/plugin.cfg")

[file_customization]

folder_colors={
"res://audio/": "gray",
"res://scen/": "red",
"res://scen/items/": "orange",
"res://scripts/": "yellow",
"res://textures/": "green",
"res://translations/": "pink"
}

[filesystem]

import/blender/enabled.android=true
import/fbx/enabled=false

[global_group]

damage=""
enemy=""
light="light"
random_points=""

[importer_defaults]

animation_library={
"animation/fps": 120.0
}

